{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
      "src": "apps/www/package.json",
      "use": "@vercel/next",
      "config": {
        "buildCommand": "cd ../.. && pnpm install --frozen-lockfile && pnpm --filter=www vercel:build:safe"
      }
    }
  ],
  "env": {
    "SKIP_ENV_VALIDATION": "1",
    "NODE_ENV": "production"
  }
}