{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd apps/www && pnpm vercel:build",
  "outputDirectory": "apps/www/.next",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": "nextjs",
  "functions": {
    "apps/www/app/**/*.{js,ts,tsx}": {
      "maxDuration": 30
    }
  },
  "env": {
    "SKIP_ENV_VALIDATION": "1",
    "NODE_ENV": "production"
  },
  "regions": ["iad1"]
} 